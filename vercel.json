{
  "version": 2,
  "builds": [
    {
      "src": "UI/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "UI/dist"
      }
    },
    {
      "src": "API/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "API/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}